<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/petfoodtitle"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_food">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="172dp"
        android:layout_height="186dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/btndogfood"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textCategory2"
        app:srcCompat="@drawable/dogfood" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="172dp"
        android:layout_height="186dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/birdfood"
        tools:layout_editor_absoluteY="144dp" />

    <Button
        android:id="@+id/btndogfood"
        android:layout_width="175dp"
        android:layout_height="61dp"
        android:layout_marginStart="8dp"
        android:text="@string/dog_food"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="364dp" />

    <Button
        android:id="@+id/btnfishfood"
        android:layout_width="175dp"
        android:layout_height="61dp"
        android:layout_marginEnd="8dp"
        android:text="@string/bird_food"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="364dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="172dp"
        android:layout_height="186dp"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/catfood"
        tools:layout_editor_absoluteY="452dp" />

    <Button
        android:id="@+id/btncatfood"
        android:layout_width="175dp"
        android:layout_height="61dp"
        android:layout_marginTop="16dp"
        android:text="@string/bird_food"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        tools:layout_editor_absoluteX="8dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="172dp"
        android:layout_height="186dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/fishfood"
        tools:layout_editor_absoluteY="452dp" />

    <Button
        android:id="@+id/btnfishfood"
        android:layout_width="175dp"
        android:layout_height="61dp"
        android:text="@string/fish_food"
        tools:layout_editor_absoluteX="220dp"
        tools:layout_editor_absoluteY="654dp" />

    <TextView
        android:id="@+id/textCategory2"
        android:layout_width="305dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:text="@string/food_cat"
        android:textAlignment="center"
        android:textColor="#606BB1"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="70dp" />
</androidx.constraintlayout.widget.ConstraintLayout>